MAP (@P<) TARGET NOUN-NOT-VN IF (*-1 (Prep Poss) BARRIER (Noun) OR (@P<));
# Handle pre-mods on np after prep phrase: idir thuas dhá sheamair
MAP (@P<) TARGET NOUN-NOT-VN IF (*-1 (Prep Simp) BARRIER (Noun) OR (Rel) OR (@P<));
MAP (@P<) TARGET NOUN-NOT-VN IF (-1 (@PP_NEG));
MAP (@P<) TARGET NOUN-NOT-VN IF (-1 PRENOM) (-2 (@PP_ADVL)) (NOT -2 (Pron Prep));
MAP (@P<) TARGET NOUN-NOT-VN IF (-1 PRENOM) (-2 PRENOM) (-3 (@PP_ADVL)) (NOT -3 (Pron Prep));
